<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false"><!--没点击按钮的时候-->
        <shape android:shape="rectangle"><!--按钮形状-->
            <solid android:color="@color/bg_tab_security" /><!--按钮背景填充色-->
            <stroke
                android:width="1dp"
                android:color="@color/bg_tab_security" /><!--按钮边框-->
        </shape>
    </item>
    <item android:drawable="@color/bg_tab_security_checked" android:state_pressed="true" />
</selector>